  "version": "1.1.0",
  "type": "module",
  "description": "API contracts, shared types, and validation schemas for abe-stack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./domains/auth": {
      "types": "./dist/domains/auth/index.d.ts",
      "default": "./dist/domains/auth/index.js"
    },
    "./domains/cache": {
      "types": "./dist/domains/cache/index.d.ts",
      "default": "./dist/domains/cache/index.js"
    },
    "./domains/users": {
      "types": "./dist/domains/users/index.d.ts",
