  "homepage": "https://github.com/abrahamahn/abe-stack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "dependencies": {
    "@abe-stack/contracts": "workspace:*",
    "postgres": "^3.4.8",
    "zod": "^4.3.6"
  }
