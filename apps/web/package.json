{
  "name": "@abe-stack/web",
  "version": "1.0.0",
  "description": "React web application for abe-stack",
  "type": "module",
  "scripts": {
    "dev": "tsx ../../tooling/scripts/dev/kill-port.ts 5173 && vite --config ../../.config/vite.web.config.ts --open",
