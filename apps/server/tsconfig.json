      "@config/*": ["./src/config/*"],
      "@crypto": ["./src/infrastructure/security/crypto"],
      "@crypto/*": ["./src/infrastructure/security/crypto/*"],
      "@data": ["./src/infrastructure/data"],
      "@data/*": ["./src/infrastructure/data/*"],
