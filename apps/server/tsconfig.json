{
  "extends": "../../.config/tsconfig.node.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "rootDir": "./",
    "composite": true,
    "noEmit": false,
    "tsBuildInfoFile": "./node_modules/.cache/typescript/tsconfig.server.tsbuildinfo",
    "paths": {
      "@/*": [
            "./src/*"
      ],
      "@abe-stack/auth": [
            "../../packages/auth/src/index.ts"
      ],
      "@abe-stack/auth/*": [
            "../../packages/auth/src/*"
      ],
      "@abe-stack/billing": [
            "../../packages/billing/src/index.ts"
      ],
      "@abe-stack/billing/*": [
            "../../packages/billing/src/*"
      ],
      "@abe-stack/cache": [
            "../../packages/cache/src/index.ts"
      ],
      "@abe-stack/cache/*": [
            "../../packages/cache/src/*"
      ],
      "@abe-stack/contracts": [
            "../../packages/contracts/src/index.ts"
      ],
      "@abe-stack/contracts/*": [
            "../../packages/contracts/src/*"
      ],
      "@abe-stack/core": [
            "../../packages/core/src/index.ts"
      ],
      "@abe-stack/email": [
            "../../packages/email/src/index.ts"
      ],
      "@abe-stack/http": [
            "../../packages/http/src/index.ts"
      ],
      "@abe-stack/http/*": [
            "../../packages/http/src/*"
      ],
      "@abe-stack/core/pubsub/postgres": [
            "../../packages/core/src/infrastructure/pubsub/postgres-pubsub.ts"
      ],
      "@abe-stack/core/pubsub": [
            "../../packages/core/src/infrastructure/pubsub/index.ts"
      ],
      "@abe-stack/core/*": [
            "../../packages/core/src/*"
      ],
      "@abe-stack/db": [
            "../../packages/db/src/index.ts"
      ],
      "@abe-stack/db/*": [
            "../../packages/db/src/*"
      ],
      "@abe-stack/jobs": [
            "../../packages/jobs/src/index.ts"
      ],
      "@abe-stack/jobs/*": [
            "../../packages/jobs/src/*"
      ],
      "@abe-stack/media": [
            "../../packages/media/src/index.ts"
      ],
      "@abe-stack/media/*": [
            "../../packages/media/src/*"
      ],
      "@abe-stack/notifications": [
            "../../packages/notifications/src/index.ts"
      ],
      "@abe-stack/notifications/*": [
            "../../packages/notifications/src/*"
      ],
      "@abe-stack/realtime": [
            "../../packages/realtime/src/index.ts"
      ],
      "@abe-stack/realtime/*": [
            "../../packages/realtime/src/*"
      ],
      "@abe-stack/security": [
            "../../packages/security/src/index.ts"
      ],
      "@abe-stack/security/*": [
            "../../packages/security/src/*"
      ],
      "@abe-stack/storage": [
            "../../packages/storage/src/index.ts"
      ],
      "@abe-stack/storage/*": [
            "../../packages/storage/src/*"
      ],
      "@abe-stack/users": [
            "../../packages/users/src/index.ts"
      ],
      "@abe-stack/users/*": [
            "../../packages/users/src/*"
      ],
      "@admin": [
            "./src/modules/admin"
      ],
      "@admin/*": [
            "./src/modules/admin/*"
      ],
      "@billing": [
            "./src/modules/billing"
      ],
      "@billing/*": [
            "./src/modules/billing/*"
      ],
      "@config": [
            "./src/config"
      ],
      "@config/*": [
            "./src/config/*"
      ],
      "@http": [
            "./src/infrastructure/http"
      ],
      "@http/*": [
            "./src/infrastructure/http/*"
      ],
      "@jobs": [
            "./src/infrastructure/jobs"
      ],
      "@jobs/*": [
            "./src/infrastructure/jobs/*"
      ],
      "@media": [
            "./src/infrastructure/media"
      ],
      "@media/*": [
            "./src/infrastructure/media/*"
      ],
      "@messaging": [
            "./src/infrastructure/messaging"
      ],
      "@messaging/*": [
            "./src/infrastructure/messaging/*"
      ],
      "@monitor": [
            "./src/infrastructure/monitor"
      ],
      "@monitor/*": [
            "./src/infrastructure/monitor/*"
      ],
      "@search": [
            "./src/infrastructure/search"
      ],
      "@search/*": [
            "./src/infrastructure/search/*"
      ],
      "@security": [
            "./src/infrastructure/security"
      ],
      "@security/*": [
            "./src/infrastructure/security/*"
      ],
      "@shared": [
            "./src/shared"
      ],
      "@shared/*": [
            "./src/shared/*"
      ],
      "@system": [
            "./src/modules/system"
      ],
      "@system/*": [
            "./src/modules/system/*"
      ],
      "@types": [
            "./src/types"
      ],
      "@types/*": [
            "./src/types/*"
      ],
      "@users": [
            "./src/modules/users"
      ],
      "@users/*": [
            "./src/modules/users/*"
      ],
      "@utils": [
            "./src/utils"
      ],
      "@utils/*": [
            "./src/utils/*"
      ]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "**/*.test.ts", "**/*.test.tsx", "**/__tests__/**"],
  "references": [
    { "path": "../../packages/auth" },
    { "path": "../../packages/billing" },
    { "path": "../../packages/cache" },
    { "path": "../../packages/core" },
    { "path": "../../packages/contracts" },
    { "path": "../../packages/db" },
    { "path": "../../packages/email" },
    { "path": "../../packages/http" },
    { "path": "../../packages/jobs" },
    { "path": "../../packages/media" },
    { "path": "../../packages/notifications" },
    { "path": "../../packages/realtime" },
    { "path": "../../packages/security" },
    { "path": "../../packages/storage" },
    { "path": "../../packages/users" }
  ]
}
