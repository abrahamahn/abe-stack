{
  "extends": "../../.config/tsconfig.node.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "rootDir": "./",
    "composite": true,
    "noEmit": false,
    "tsBuildInfoFile": "./node_modules/.cache/typescript/tsconfig.server.tsbuildinfo",
    "paths": {
      "@/*": [
            "./src/*"
      ],
      "@abe-stack/contracts": [
            "../../packages/contracts/src/index.ts"
      ],
      "@abe-stack/contracts/*": [
            "../../packages/contracts/src/*"
      ],
      "@abe-stack/core": [
            "../../packages/core/src/index.ts"
      ],
      "@abe-stack/core/*": [
            "../../packages/core/src/*"
      ],
      "@abe-stack/db": [
            "../../packages/db/src/index.ts"
      ],
      "@abe-stack/db/*": [
            "../../packages/db/src/*"
      ],
      "@abe-stack/media": [
            "../../packages/media/src/index.ts"
      ],
      "@abe-stack/media/*": [
            "../../packages/media/src/*"
      ],
      "@admin": [
            "./src/modules/admin"
      ],
      "@admin/*": [
            "./src/modules/admin/*"
      ],
      "@auth": [
            "./src/modules/auth"
      ],
      "@auth/*": [
            "./src/modules/auth/*"
      ],
      "@billing": [
            "./src/modules/billing"
      ],
      "@billing/*": [
            "./src/modules/billing/*"
      ],
      "@cache": [
            "./src/infrastructure/cache"
      ],
      "@cache/*": [
            "./src/infrastructure/cache/*"
      ],
      "@crypto": [
            "./src/infrastructure/security/crypto"
      ],
      "@crypto/*": [
            "./src/infrastructure/security/crypto/*"
      ],
      "@config": [
            "./src/config"
      ],
      "@config/*": [
            "./src/config/*"
      ],
      "@data": [
            "./src/infrastructure/data"
      ],
      "@data/*": [
            "./src/infrastructure/data/*"
      ],
      "@database": [
            "./src/infrastructure/data/database"
      ],
      "@database/*": [
            "./src/infrastructure/data/database/*"
      ],
      "@email": [
            "./src/infrastructure/messaging/email"
      ],
      "@email/*": [
            "./src/infrastructure/messaging/email/*"
      ],
      "@http": [
            "./src/infrastructure/http"
      ],
      "@http/*": [
            "./src/infrastructure/http/*"
      ],
      "@infrastructure": [
            "./src/infrastructure"
      ],
      "@infrastructure/*": [
            "./src/infrastructure/*"
      ],
      "@jobs": [
            "./src/infrastructure/jobs"
      ],
      "@jobs/*": [
            "./src/infrastructure/jobs/*"
      ],
      "@media": [
            "./src/infrastructure/media"
      ],
      "@media/*": [
            "./src/infrastructure/media/*"
      ],
      "@messaging": [
            "./src/infrastructure/messaging"
      ],
      "@messaging/*": [
            "./src/infrastructure/messaging/*"
      ],
      "@logger": [
            "./src/infrastructure/monitor/logger"
      ],
      "@logger/*": [
            "./src/infrastructure/monitor/logger/*"
      ],
      "@monitor": [
            "./src/infrastructure/monitor"
      ],
      "@monitor/*": [
            "./src/infrastructure/monitor/*"
      ],
      "@notifications": [
            "./src/modules/notifications"
      ],
      "@notifications/*": [
            "./src/modules/notifications/*"
      ],
      "@modules": [
            "./src/modules"
      ],
      "@modules/*": [
            "./src/modules/*"
      ],
      "@realtime": [
            "./src/modules/realtime"
      ],
      "@realtime/*": [
            "./src/modules/realtime/*"
      ],
      "@search": [
            "./src/infrastructure/search"
      ],
      "@search/*": [
            "./src/infrastructure/search/*"
      ],
      "@security": [
            "./src/infrastructure/security"
      ],
      "@security/*": [
            "./src/infrastructure/security/*"
      ],
      "@shared": [
            "./src/shared"
      ],
      "@shared/*": [
            "./src/shared/*"
      ],
      "@pagination": [
            "./src/infrastructure/http/pagination"
      ],
      "@pagination/*": [
            "./src/infrastructure/http/pagination/*"
      ],
      "@rate-limit": [
            "./src/infrastructure/security/rate-limit"
      ],
      "@rate-limit/*": [
            "./src/infrastructure/security/rate-limit/*"
      ],
      "@router": [
            "./src/infrastructure/http/router"
      ],
      "@router/*": [
            "./src/infrastructure/http/router/*"
      ],
      "@storage": [
            "./src/infrastructure/data/storage"
      ],
      "@storage/*": [
            "./src/infrastructure/data/storage/*"
      ],
      "@system": [
            "./src/modules/system"
      ],
      "@system/*": [
            "./src/modules/system/*"
      ],
      "@types": [
            "./src/types"
      ],
      "@types/*": [
            "./src/types/*"
      ],
      "@users": [
            "./src/modules/users"
      ],
      "@users/*": [
            "./src/modules/users/*"
      ],
      "@utils": [
            "./src/utils"
      ],
      "@utils/*": [
            "./src/utils/*"
      ]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "**/*.test.ts", "**/*.test.tsx", "**/__tests__/**"],
  "references": [
    { "path": "../../packages/core" },
    { "path": "../../packages/db" },
    { "path": "../../packages/media" },
    { "path": "../../packages/contracts" }
  ]
}
