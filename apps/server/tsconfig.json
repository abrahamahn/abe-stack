    "tsBuildInfoFile": "./node_modules/.cache/tsconfig.server.tsbuildinfo"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"],
  "references": [
    {
      "path": "../../packages/core"
    }
  ]
