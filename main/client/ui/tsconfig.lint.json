{
  "extends": "../../../config/tsconfig.react.json",
  "compilerOptions": {
    "composite": false,
    "noEmit": true,
    "declaration": false,
    "declarationMap": false,
    "jsx": "react-jsx",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "baseUrl": "./",
    "paths": {
      "@bslt/shared": ["../../shared/src/index.ts"],
      "@bslt/shared/*": ["../../shared/src/*"],
      "@bslt/api": ["../api/src/index.ts"],
      "@bslt/api/*": ["../api/src/*"],
      "@bslt/client-engine": ["../engine/src/index.ts"],
      "@bslt/client-engine/*": ["../engine/src/*"],
      "@bslt/react": ["../react/src/index.ts"],
      "@bslt/react/*": ["../react/src/*"],
      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],
      "@containers": ["./src/layouts/containers"],
      "@containers/*": ["./src/layouts/containers/*"],
      "@elements": ["./src/elements"],
      "@elements/*": ["./src/elements/*"],
      "@hooks": ["../react/src/hooks"],
      "@hooks/*": ["../react/src/hooks/*"],
      "@layers": ["./src/layouts/layers"],
      "@layers/*": ["./src/layouts/layers/*"],
      "@layouts": ["./src/layouts"],
      "@layouts/*": ["./src/layouts/*"],
      "@providers": ["../react/src/providers"],
      "@providers/*": ["../react/src/providers/*"],
      "@router": ["../react/src/router"],
      "@router/*": ["../react/src/router/*"],
      "@shells": ["./src/layouts/shells"],
      "@shells/*": ["./src/layouts/shells/*"],
      "@theme": ["./src/theme"],
      "@theme/*": ["./src/theme/*"],
      "@types": ["./src/types"],
      "@types/*": ["./src/types/*"],
      "@utils": ["./src/utils"],
      "@utils/*": ["./src/utils/*"]
    },
    "tsBuildInfoFile": "./node_modules/.cache/typescript/tsconfig.ui.lint.tsbuildinfo"
  },
  "include": [
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "src/**/__tests__/**/*"
  ],
  "exclude": ["node_modules", "dist"]
}
