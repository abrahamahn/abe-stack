{
  "extends": "../../../config/tsconfig.react.json",
  "compilerOptions": {
    "paths": {
      "@bslt/shared": ["../../shared/src/index.ts"],
      "@bslt/shared/*": ["../../shared/src/*"],
      "@bslt/client-engine": ["../../client/engine/src/index.ts"],
      "@bslt/client-engine/*": ["../../client/engine/src/*"],
      "@bslt/api": ["../../client/api/src/index.ts"],
      "@bslt/api/*": ["../../client/api/src/*"],
      "@bslt/ui": ["../../client/ui/src/index.ts"],
      "@bslt/ui/*": ["../../client/ui/src/*"],
      "@/*": ["../web/src/*"],
      "@admin": ["../web/src/features/admin"],
      "@admin/*": ["../web/src/features/admin/*"],
      "@api": ["../web/src/api"],
      "@api/*": ["../web/src/api/*"],
      "@app": ["../web/src/app"],
      "@app/*": ["../web/src/app/*"],
      "@auth": ["../web/src/features/auth"],
      "@auth/*": ["../web/src/features/auth/*"],
      "@billing": ["../web/src/features/billing"],
      "@billing/*": ["../web/src/features/billing/*"],
      "@catalog": ["../web/src/features/ui-library/catalog"],
      "@catalog/*": ["../web/src/features/ui-library/catalog/*"],
      "@components": ["../web/src/features/admin/components"],
      "@components/*": ["../web/src/features/admin/components/*"],
      "@config": ["../web/src/config"],
      "@config/*": ["../web/src/config/*"],
      "@dashboard": ["../web/src/features/dashboard"],
      "@dashboard/*": ["../web/src/features/dashboard/*"],
      "@ui-library": ["../web/src/features/ui-library"],
      "@ui-library/*": ["../web/src/features/ui-library/*"],
      "@features": ["../web/src/features"],
      "@features/*": ["../web/src/features/*"],
      "@home": ["../web/src/features/home"],
      "@home/*": ["../web/src/features/home/*"],
      "@hooks": ["../web/src/features/admin/hooks"],
      "@hooks/*": ["../web/src/features/admin/hooks/*"],
      "@layouts": ["../web/src/features/admin/layouts"],
      "@layouts/*": ["../web/src/features/admin/layouts/*"],
      "@pages": ["../web/src/pages"],
      "@pages/*": ["../web/src/pages/*"],
      "@services": ["../web/src/features/admin/services"],
      "@services/*": ["../web/src/features/admin/services/*"],
      "@settings": ["../web/src/features/settings"],
      "@settings/*": ["../web/src/features/settings/*"],
      "@desktop/*": ["./src/*"],
      "@ipc": ["./src/electron/ipc"],
      "@ipc/*": ["./src/electron/ipc/*"]
    },
    "outDir": "./dist",
    "composite": true,
    "noEmit": false,
    "tsBuildInfoFile": "./node_modules/.cache/typescript/tsconfig.desktop.tsbuildinfo",
    "rootDir": "../../../"
  },
  "include": ["src/**/*", "../web/src/**/*"],
  "exclude": [
    "node_modules",
    "vitest.config.ts",
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/__tests__/**/*",
    "../web/src/__tests__/**/*",
    "../web/src/**/*.test.ts",
    "../web/src/**/*.test.tsx"
  ],
  "references": [
    {
      "path": "../../shared"
    },
    {
      "path": "../../client/ui"
    },
    {
      "path": "../../client/api"
    }
  ]
}
