{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": false,
    "noEmit": true,
    "rootDir": "../..",
    "paths": {
      "@/*": ["./src/*"],
      "@admin": ["./src/features/admin"],
      "@admin/*": ["./src/features/admin/*"],
      "@api": ["./src/api"],
      "@api/*": ["./src/api/*"],
      "@app": ["./src/app"],
      "@app/*": ["./src/app/*"],
      "@auth": ["./src/features/auth"],
      "@auth/*": ["./src/features/auth/*"],
      "@bslt/api": ["../../client/api/src/index.ts"],
      "@bslt/api/*": ["../../client/api/src/*"],
      "@bslt/client-engine": ["../../client/engine/src/index.ts"],
      "@bslt/client-engine/*": ["../../client/engine/src/*"],
      "@bslt/react": ["../../client/react/src/index.ts"],
      "@bslt/react/*": ["../../client/react/src/*"],
      "@bslt/shared": ["../../shared/src/index.ts"],
      "@bslt/shared/*": ["../../shared/src/*"],
      "@bslt/ui": ["../../client/ui/src/index.ts"],
      "@bslt/ui/*": ["../../client/ui/src/*"],
      "@billing": ["./src/features/billing"],
      "@billing/*": ["./src/features/billing/*"],
      "@catalog": ["./src/features/ui-library/catalog"],
      "@catalog/*": ["./src/features/ui-library/catalog/*"],
      "@components": ["../../client/ui/src/components", "./src/features/admin/components"],
      "@components/*": ["../../client/ui/src/components/*", "./src/features/admin/components/*"],
      "@config": ["./src/config"],
      "@config/*": ["./src/config/*"],
      "@dashboard": ["./src/features/dashboard"],
      "@dashboard/*": ["./src/features/dashboard/*"],
      "@ui-library": ["./src/features/ui-library"],
      "@ui-library/*": ["./src/features/ui-library/*"],
      "@features": ["./src/features"],
      "@features/*": ["./src/features/*"],
      "@home": ["./src/features/home"],
      "@home/*": ["./src/features/home/*"],
      "@hooks": ["../../client/react/src/hooks"],
      "@hooks/*": ["../../client/react/src/hooks/*"],
      "@providers": ["../../client/react/src/providers"],
      "@providers/*": ["../../client/react/src/providers/*"],
      "@router": ["../../client/react/src/router"],
      "@router/*": ["../../client/react/src/router/*"],
      "@containers": ["../../client/ui/src/layouts/containers"],
      "@containers/*": ["../../client/ui/src/layouts/containers/*"],
      "@elements": ["../../client/ui/src/elements"],
      "@elements/*": ["../../client/ui/src/elements/*"],
      "@layers": ["../../client/ui/src/layouts/layers"],
      "@layers/*": ["../../client/ui/src/layouts/layers/*"],
      "@layouts": ["../../client/ui/src/layouts", "./src/app/layouts"],
      "@layouts/*": ["../../client/ui/src/layouts/*", "./src/app/layouts/*"],
      "@pages": ["./src/pages"],
      "@pages/*": ["./src/pages/*"],
      "@services": ["./src/features/admin/services"],
      "@services/*": ["./src/features/admin/services/*"],
      "@settings": ["./src/features/settings"],
      "@settings/*": ["./src/features/settings/*"],
      "@shells": ["../../client/ui/src/layouts/shells"],
      "@shells/*": ["../../client/ui/src/layouts/shells/*"],
      "@theme": ["../../client/ui/src/theme"],
      "@theme/*": ["../../client/ui/src/theme/*"],
      "@types": ["../../client/ui/src/types"],
      "@types/*": ["../../client/ui/src/types/*"],
      "@utils": ["../../client/ui/src/utils"],
      "@utils/*": ["../../client/ui/src/utils/*"]
    },
    "declaration": false,
    "declarationMap": false
  },
  "references": []
}
