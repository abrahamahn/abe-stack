{
  "extends": "../../../config/tsconfig.react.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./src/*"],
      "@admin": ["./src/features/admin"],
      "@admin/*": ["./src/features/admin/*"],
      "@api": ["./src/api"],
      "@api/*": ["./src/api/*"],
      "@app": ["./src/app"],
      "@app/*": ["./src/app/*"],
      "@auth": ["./src/features/auth"],
      "@auth/*": ["./src/features/auth/*"],
      "@abe-stack/api": ["../../client/api/src/index.ts"],
      "@abe-stack/api/*": ["../../client/api/src/*"],
      "@abe-stack/client-engine": ["../../client/engine/src/index.ts"],
      "@abe-stack/client-engine/*": ["../../client/engine/src/*"],
      "@abe-stack/react": ["../../client/react/src/index.ts"],
      "@abe-stack/react/*": ["../../client/react/src/*"],
      "@abe-stack/shared": ["../../shared/src/index.ts"],
      "@abe-stack/shared/*": ["../../shared/src/*"],
      "@abe-stack/ui": ["../../client/ui/src/index.ts"],
      "@abe-stack/ui/*": ["../../client/ui/src/*"],
      "@billing": ["./src/features/billing"],
      "@billing/*": ["./src/features/billing/*"],
      "@catalog": ["./src/features/ui-library/catalog"],
      "@catalog/*": ["./src/features/ui-library/catalog/*"],
      "@components": ["./src/features/admin/components"],
      "@components/*": ["./src/features/admin/components/*"],
      "@config": ["./src/config"],
      "@config/*": ["./src/config/*"],
      "@dashboard": ["./src/features/dashboard"],
      "@dashboard/*": ["./src/features/dashboard/*"],
      "@ui-library": ["./src/features/ui-library"],
      "@ui-library/*": ["./src/features/ui-library/*"],
      "@features": ["./src/features"],
      "@features/*": ["./src/features/*"],
      "@home": ["./src/features/home"],
      "@home/*": ["./src/features/home/*"],
      "@hooks": ["../../client/react/src/hooks"],
      "@hooks/*": ["../../client/react/src/hooks/*"],
      "@providers": ["../../client/react/src/providers"],
      "@providers/*": ["../../client/react/src/providers/*"],
      "@router": ["../../client/react/src/router"],
      "@router/*": ["../../client/react/src/router/*"],
      "@layouts": ["./src/app/layouts"],
      "@layouts/*": ["./src/app/layouts/*"],
      "@pages": ["./src/pages"],
      "@pages/*": ["./src/pages/*"],
      "@services": ["./src/features/admin/services"],
      "@services/*": ["./src/features/admin/services/*"],
      "@settings": ["./src/features/settings"],
      "@settings/*": ["./src/features/settings/*"]
    },
    "outDir": "./dist",
    "rootDir": "./",
    "types": ["vite/client"],
    "composite": true,
    "noEmit": false,
    "tsBuildInfoFile": "./node_modules/.cache/typescript/tsconfig.web.tsbuildinfo"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "vitest.config.ts"],
  "references": [
    {
      "path": "../../shared"
    },
    {
      "path": "../../server/engine"
    },
    {
      "path": "../../server/media"
    },
    {
      "path": "../../client/engine"
    },
    {
      "path": "../../client/api"
    },
    {
      "path": "../../client/ui"
    },
    {
      "path": "../../client/react"
    }
  ]
}
