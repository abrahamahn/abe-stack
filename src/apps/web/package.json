{
  "name": "@abe-stack/web",
  "version": "1.0.0",
  "description": "React web application for abe-stack",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "start": "vite preview --port 4173",
    "api:index": "node scripts/generate-api-index.mjs",
    "type-check": "tsc --noEmit",
    "test": "vitest run --reporter=dot --silent=passed-only",
    "lint": "eslint . --cache --cache-location ../../node_modules/.cache/eslint/.eslintcache-abe-stack-web --report-unused-disable-directives",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@abe-stack/api": "workspace:*",
    "@abe-stack/client-engine": "workspace:*",
    "@abe-stack/ui": "workspace:*",
    "@abe-stack/react": "workspace:*",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "@abe-stack/shared": "workspace:*"
  },
  "devDependencies": {
    "vite-tsconfig-paths": "^6.0.5",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3"
  }
}
