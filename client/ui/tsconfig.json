{
  "extends": "../../.config/tsconfig.react.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "jsx": "react-jsx",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "baseUrl": "./",
    "types": [
      "react",
      "react-dom",
      "vitest/globals",
      "@testing-library/jest-dom",
      "node"
    ],
    "paths": {
      "@components": [
        "./src/components"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@containers": [
        "./src/layouts/containers"
      ],
      "@containers/*": [
        "./src/layouts/containers/*"
      ],
      "@elements": [
        "./src/elements"
      ],
      "@elements/*": [
        "./src/elements/*"
      ],
      "@hooks": [
        "../../client/react/src/hooks"
      ],
      "@hooks/*": [
        "../../client/react/src/hooks/*"
      ],
      "@layers": [
        "./src/layouts/layers"
      ],
      "@layers/*": [
        "./src/layouts/layers/*"
      ],
      "@layouts": [
        "./src/layouts"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@providers": [
        "../../client/react/src/providers"
      ],
      "@providers/*": [
        "../../client/react/src/providers/*"
      ],
      "@router": [
        "../../client/react/src/router"
      ],
      "@router/*": [
        "../../client/react/src/router/*"
      ],
      "@shells": [
        "./src/layouts/shells"
      ],
      "@shells/*": [
        "./src/layouts/shells/*"
      ],
      "@theme": [
        "./src/theme"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@types": [
        "./src/types"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    },
    "tsBuildInfoFile": "./node_modules/.cache/typescript/tsconfig.ui.tsbuildinfo"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "vitest.config.ts"
  ],
  "references": [
    {
      "path": "../../packages/shared"
    },
    {
      "path": "../../premium/client/engine"
    },
    {
      "path": "../../client/react"
    }
  ]
}
