{
  "name": "@abe-stack/sdk",
  "version": "1.1.0",
  "description": "SDK for abe-stack API (web/desktop/mobile)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.js"
    },
    "./react-query": {
      "types": "./dist/react-query.d.ts",
      "import": "./dist/react-query.js",
      "require": "./dist/react-query.js"
    },
    "./persistence": {
      "types": "./dist/persistence/index.d.ts",
      "import": "./dist/persistence/index.js",
      "require": "./dist/persistence/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --watch --project tsconfig.json",
    "clean": "rm -rf dist",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "test": "VITEST_TARGET=sdk vitest run --config ../../config/vitest.config.ts"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/abrahamahn/abe-stack.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/abrahamahn/abe-stack/issues"
  },
  "homepage": "https://github.com/abrahamahn/abe-stack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "dependencies": {
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/react-query": "^3.52.1",
    "@tanstack/react-query": "^5.59.9",
    "@tanstack/query-persist-client-core": "^5.59.9",
    "@abe-stack/core": "workspace:*",
    "idb-keyval": "^6.2.1"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
