{
  "name": "@abe-stack/media",
  "version": "1.0.0",
  "type": "module",
  "description": "Media processing utilities for abe-stack (server-only)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "sharp": "^0.33.2"
  },
  "scripts": {
    "build": "[ -d dist ] || rm -f tsconfig.tsbuildinfo && tsc --build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "type-check": "tsc --noEmit",
    "test": "vitest run --reporter=dot",
    "lint": "eslint . --cache --cache-location ../../node_modules/.cache/eslint/.eslintcache-abe-stack-media --report-unused-disable-directives"
  },
  "keywords": [
    "abe-stack",
    "media",
    "ffmpeg",
    "image-processing"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/abrahamahn/abe-stack.git",
    "directory": "packages/media"
  },
  "bugs": {
    "url": "https://github.com/abrahamahn/abe-stack/issues"
  },
  "homepage": "https://github.com/abrahamahn/abe-stack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false
}
