{
  "name": "@abe-stack/storage",
  "version": "1.0.0",
  "description": "Pluggable storage providers (local & S3) for ABE Stack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "sideEffects": false,
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.677.0",
    "@aws-sdk/client-s3": "^3.677.0",
    "@aws-sdk/s3-request-presigner": "^3.677.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  }
}
