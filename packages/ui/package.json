{
  "name": "abeahn-ui",
  "version": "1.1.0",
  "description": "Shared UI components for abe-stack (web, desktop, mobile)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./primitives": {
      "import": "./dist/primitives/index.js",
      "types": "./dist/primitives/index.d.ts"
    },
    "./layouts": {
      "import": "./dist/layouts/index.js",
      "types": "./dist/layouts/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./theme": {
      "import": "./dist/theme/index.js",
      "types": "./dist/theme/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsc --watch --project tsconfig.json",
    "clean": "rm -rf dist",
    "type-check": "tsc --project tsconfig.json --noEmit && tsc --project tsconfig.test.json --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "abe-stack",
    "ui",
    "components",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/abrahamahn/abe-stack.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/abrahamahn/abe-stack/issues"
  },
  "homepage": "https://github.com/abrahamahn/abe-stack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "dependencies": {
    "abeahn-shared": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  }
}
