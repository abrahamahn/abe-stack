{
  "extends": "../../.config/tsconfig.react.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./",
    "jsx": "react-jsx",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "baseUrl": "./",
    "types": ["react", "react-dom", "vitest/globals", "@testing-library/jest-dom", "node"],
    "paths": {
      "@abe-stack/core": [
            "../core/src"
      ],
      "@abe-stack/core/*": [
            "../core/src/*"
      ],
      "@components": [
            "./src/components"
      ],
      "@components/*": [
            "./src/components/*"
      ],
      "@containers": [
            "./src/layouts/containers"
      ],
      "@containers/*": [
            "./src/layouts/containers/*"
      ],
      "@elements": [
            "./src/elements"
      ],
      "@elements/*": [
            "./src/elements/*"
      ],
      "@hooks": [
            "./src/hooks"
      ],
      "@hooks/*": [
            "./src/hooks/*"
      ],
      "@layers": [
            "./src/layouts/layers"
      ],
      "@layers/*": [
            "./src/layouts/layers/*"
      ],
      "@layouts": [
            "./src/layouts"
      ],
      "@layouts/*": [
            "./src/layouts/*"
      ],
      "@providers": [
            "./src/providers"
      ],
      "@providers/*": [
            "./src/providers/*"
      ],
      "@router": [
            "./src/router"
      ],
      "@router/*": [
            "./src/router/*"
      ],
      "@shells": [
            "./src/layouts/shells"
      ],
      "@shells/*": [
            "./src/layouts/shells/*"
      ],
      "@theme": [
            "./src/theme"
      ],
      "@theme/*": [
            "./src/theme/*"
      ],
      "@types": [
            "./src/types"
      ],
      "@types/*": [
            "./src/types/*"
      ],
      "@utils": [
            "./src/utils"
      ],
      "@utils/*": [
            "./src/utils/*"
      ]
    },
    "tsBuildInfoFile": "./node_modules/.cache/typescript/tsconfig.ui.tsbuildinfo"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "vitest.config.ts", "**/*.test.ts", "**/*.test.tsx", "**/__tests__/**"],
  "references": [
    {
      "path": "../contracts"
    },
    {
      "path": "../core"
    },
    {
      "path": "../sdk"
    },
    {
      "path": "../stores"
    }
  ]
}
