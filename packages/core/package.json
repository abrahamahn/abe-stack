{
  "name": "@abe-stack/core",
  "version": "1.1.0",
  "description": "API contracts, shared types, and validation schemas for abe-stack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./contracts": {
      "import": "./dist/contracts/index.js",
      "require": "./dist/contracts/index.js",
      "types": "./dist/contracts/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./env": {
      "import": "./dist/env.js",
      "require": "./dist/env.js",
      "types": "./dist/env.d.ts"
    },
    "./stores": {
      "import": "./dist/stores/index.js",
      "require": "./dist/stores/index.js",
      "types": "./dist/stores/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --watch --project tsconfig.json",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "abe-stack",
    "core",
    "utilities"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/abrahamahn/abe-stack.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/abrahamahn/abe-stack/issues"
  },
  "homepage": "https://github.com/abrahamahn/abe-stack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "dependencies": {
    "@ts-rest/core": "^3.45.1",
    "nanoid": "^5.1.6",
    "zod": "^3.22.4",
    "zustand": "^5.0.9",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/zxcvbn": "^4.4.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
