{
  "name": "@abe-stack/http",
  "version": "1.0.0",
  "type": "module",
  "description": "HTTP middleware, routing, and pagination for abe-stack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "[ -d dist ] || rm -f tsconfig.tsbuildinfo && tsc --build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "type-check": "tsc --noEmit",
    "test": "vitest run --reporter=dot --silent=passed-only",
    "lint": "eslint . --cache --cache-location ../../node_modules/.cache/eslint/.eslintcache-abe-stack-http --report-unused-disable-directives"
  },
  "keywords": [
    "abe-stack",
    "http",
    "middleware",
    "router",
    "fastify"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/abrahamahn/abe-stack.git",
    "directory": "infra/http"
  },
  "bugs": {
    "url": "https://github.com/abrahamahn/abe-stack/issues"
  },
  "homepage": "https://github.com/abrahamahn/abe-stack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "dependencies": {
    "@abe-stack/contracts": "workspace:*",
    "@abe-stack/core": "workspace:*"
  },
  "peerDependencies": {
    "fastify": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "fastify": {
      "optional": true
    }
  }
}
