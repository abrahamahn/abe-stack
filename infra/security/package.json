{
  "name": "@abe-stack/security",
  "version": "1.0.0",
  "type": "module",
  "description": "Security infrastructure: JWT rotation, permissions, and rate limiting for abe-stack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./rate-limit": {
      "types": "./dist/rate-limit/index.d.ts",
      "default": "./dist/rate-limit/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "[ -d dist ] || rm -f tsconfig.tsbuildinfo && tsc --build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "type-check": "tsc --noEmit",
    "test": "vitest run --reporter=dot --silent=passed-only",
    "lint": "eslint . --cache --cache-location ../../node_modules/.cache/eslint/.eslintcache-abe-stack-security --report-unused-disable-directives"
  },
  "keywords": [
    "abe-stack",
    "security",
    "jwt",
    "permissions",
    "rate-limit"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/abrahamahn/abe-stack.git",
    "directory": "infra/security"
  },
  "bugs": {
    "url": "https://github.com/abrahamahn/abe-stack/issues"
  },
  "homepage": "https://github.com/abrahamahn/abe-stack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "dependencies": {
    "@abe-stack/core": "workspace:*"
  },
  "peerDependencies": {
    "fastify": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "fastify": {
      "optional": true
    }
  }
}
