{
  "name": "@abe-stack/notifications",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Push notification providers, subscription management, and preference handling for abe-stack",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "[ -d dist ] || rm -f node_modules/.cache/typescript/*.tsbuildinfo && tsc --build",
    "type-check": "tsc --noEmit",
    "test": "vitest run --reporter=dot --silent=passed-only",
    "test:watch": "vitest",
    "lint": "eslint . --cache --cache-location ../../node_modules/.cache/eslint/.eslintcache-abe-stack-notifications --report-unused-disable-directives"
  },
  "keywords": [
    "abe-stack",
    "notifications",
    "push",
    "fcm"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/abrahamahn/abe-stack.git",
    "directory": "infra/notifications"
  },
  "bugs": {
    "url": "https://github.com/abrahamahn/abe-stack/issues"
  },
  "homepage": "https://github.com/abrahamahn/abe-stack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "dependencies": {
    "@abe-stack/contracts": "workspace:*",
    "@abe-stack/core": "workspace:*",
    "@abe-stack/db": "workspace:*",
    "@abe-stack/http": "workspace:*"
  },
  "peerDependencies": {
    "fastify": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "fastify": {
      "optional": true
    }
  },
  "devDependencies": {
    "fastify": "5.7.1"
  }
}
