  "editor.tokenColorCustomizations": {
    "comments": "#445577",
    "functions": "#DDBBFF",
    "keywords": "#00EEFF",
    "numbers": "#80FFE5",
    "strings": "#05D3FF",
    "types": "#99BBFF",
    "variables": "#AACCFF",
    "textMateRules": [
      { "scope": "comment", "settings": { "foreground": "#445577", "fontStyle": "italic" } },
      { "scope": "string", "settings": { "foreground": "#05D3FF" } },
      { "scope": "keyword", "settings": { "foreground": "#00EEFF", "fontStyle": "bold" } },
      { "scope": "keyword.control", "settings": { "foreground": "#00EEFF", "fontStyle": "bold" } },
      { "scope": "keyword.operator", "settings": { "foreground": "#AACCFF" } },
      { "scope": "storage", "settings": { "foreground": "#00EEFF" } },
      { "scope": "storage.type", "settings": { "foreground": "#00EEFF" } },
      { "scope": "entity.name.function", "settings": { "foreground": "#DDBBFF" } },
      { "scope": "entity.name.type", "settings": { "foreground": "#99BBFF" } },
      { "scope": "entity.name.class", "settings": { "foreground": "#99BBFF", "fontStyle": "bold" } },
      { "scope": "entity.other.attribute-name", "settings": { "foreground": "#AACCFF" } },
      { "scope": "variable", "settings": { "foreground": "#AACCFF" } },
      { "scope": "variable.parameter", "settings": { "foreground": "#66BBFF" } },
      { "scope": "variable.other.constant", "settings": { "foreground": "#80FFE5" } },
      { "scope": "constant", "settings": { "foreground": "#80FFE5" } },
      { "scope": "constant.numeric", "settings": { "foreground": "#80FFE5" } },
      { "scope": "constant.language", "settings": { "foreground": "#00EEFF", "fontStyle": "italic" } },
      { "scope": "support.function", "settings": { "foreground": "#DDBBFF" } },
      { "scope": "support.type", "settings": { "foreground": "#99BBFF" } },
      { "scope": "meta.tag", "settings": { "foreground": "#AACCFF" } },
      { "scope": "entity.name.tag", "settings": { "foreground": "#00EEFF" } },
      { "scope": "punctuation", "settings": { "foreground": "#336688" } },
      { "scope": "punctuation.definition.tag", "settings": { "foreground": "#336688" } },
      { "scope": "meta.brace", "settings": { "foreground": "#336688" } },
      { "scope": "markup.heading", "settings": { "foreground": "#DDBBFF", "fontStyle": "bold" } },
      { "scope": "markup.bold", "settings": { "foreground": "#00EEFF", "fontStyle": "bold" } },
      { "scope": "markup.italic", "settings": { "foreground": "#DDBBFF", "fontStyle": "italic" } },
      { "scope": "markup.inline.raw", "settings": { "foreground": "#99BBFF" } },
      { "scope": "meta.import", "settings": { "foreground": "#AACCFF" } },
      { "scope": "entity.name.module", "settings": { "foreground": "#99BBFF" } }
    ]
